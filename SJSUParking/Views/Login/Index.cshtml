@model SJSUParking.Models.LoginModel

@{
    ViewBag.Title = "SJSU Parking";
}


<h2>Welcome to SJSU Parking Login Page <br />

    Please login below for your Parking Account Information! <br /> 
        
</h2>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>


@using(Html.BeginForm())
{
    <div class="left">
            <br/> 
                <a href="/parkingpermits">Parking Permits</a> <br />
                <a href="/buypermit">Buy Permit</a>
            </div>
    <div class="main">
          <div class="content">  
    <div>
        @Html.LabelFor(m => m.SJSUId)
        @Html.TextBoxFor(m => m.SJSUId)
    </div>
    <div>
        @Html.LabelFor(m => m.Password)
        @Html.PasswordFor(m => m.Password) 
    </div> <br/>
    <div>
        <input type="submit" />
    </div> <br/>
    <div>
        @Html.ValidationSummary()
    </div>
    <div>
        
        <a href="/registration">First Time User? Please Register >></a>
    </div>
        </div>
    </div>
    
}